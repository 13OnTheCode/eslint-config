{
  "name": "@13onthecode/eslint-config",
  "description": "ESLint shareable config",
  "version": "1.1.1",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/13OnTheCode/eslint-config#readme",
  "bugs": "https://github.com/13OnTheCode/eslint-config/issues",
  "author": {
    "name": "13OnTheCode",
    "email": "13@ryongyon.com",
    "url": "https://ryongyon.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/13OnTheCode/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "javascript",
    "typescript",
    "node",
    "react"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "^16.0.0 || >=18.0.0"
  },
  "packageManager": "pnpm@8.6.6",
  "scripts": {
    "build": "unbuild",
    "build:all": "pnpm -r run build && pnpm run build",
    "clean": "pnpm dlx rimraf node_modules dist",
    "clean:all": "pnpm -r run clean && pnpm run clean"
  },
  "dependencies": {
    "@13onthecode/eslint-config-javascript": "workspace:*",
    "@13onthecode/eslint-config-node": "workspace:*",
    "@13onthecode/eslint-config-react": "workspace:*",
    "@13onthecode/eslint-config-typescript": "workspace:*"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "unbuild": "^1.2.1"
  },
  "peerDependencies": {
    "eslint": "^7.0.0 || ^8.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "files": [
    "dist",
    "LICENSE.md",
    "CHANGELOG.md",
    "README.md",
    "README.CN.md"
  ],
  "unbuild": {
    "rollup": {
      "esbuild": {
        "minify": true
      }
    }
  }
}
